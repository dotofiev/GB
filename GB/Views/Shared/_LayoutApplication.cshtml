<!DOCTYPE html>

<html lang="@ViewBag.Lang.code">

<head>

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"> 
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Titre de la page -->
    <title>@ViewBag.Title</title>

    <!-- icon de la page -->
    <link href="@Url.Content("~/favicon.ico")" rel="shortcut icon" type="image/ico" />

    <!-- Bootstrap -->
    <link href="@Url.Content("~/Resources/css/bootstrap.min.css")" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="@Url.Content("~/Plugins/font-awesome/css/font-awesome.min.css")" rel="stylesheet" />
    <!-- NProgress -->
    @*<link href="@Url.Content("~/Plugins/nprogress/nprogress.css")" rel="stylesheet" />*@
    <!-- EasyAutocomplete-1.3.5 -->
    <link href="@Url.Content("~/Plugins/EasyAutocomplete-1.3.5/easy-autocomplete.min.css")" rel="stylesheet" />
    <!-- jQuery custom content scroller -->
    <link href="@Url.Content("~/Plugins/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css")" rel="stylesheet" />
    <!-- iCheck -->
    <link href="@Url.Content("~/Plugins/iCheck/skins/flat/green.css")" rel="stylesheet" />
    <!-- Notify -->
    <link href="@Url.Content("~/Plugins/notify/pnotity.css")" rel="stylesheet" type="text/css" />
    <!-- datatables -->
    <link href="@Url.Content("~/Plugins/datatables/datatables/jquery.dataTables.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Plugins/datatables/datatables/buttons.bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Plugins/datatables/datatables/fixedHeader.bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Plugins/datatables/datatables/responsive.bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Plugins/datatables/datatables/scroller.bootstrap.min.css")" rel="stylesheet" />
    <!-- Custom Theme Style -->
    <link href="@Url.Content("~/Resources/css/custom.css")" rel="stylesheet" />
    <!-- gb.style.css -->
    <link href="@Url.Content("~/Resources/css/gb.style.css")" rel="stylesheet" />
    <!-- Style de la page -->
    <link href="@Url.Content("~/Resources/css/Views/" + ViewBag.Id_page + ".css")" rel="stylesheet" />

</head>

<body class="nav-md">

    <div class="container body">

        @RenderBody()

        <!-- Modal -->
        @Html.Partial("_Modals")

    </div>

    <!-- JSquery -->
    <script src="@Url.Content("~/Resources/js/jquery-2.2.4.min.js")" type="text/javascript"></script>
    <!-- Boostrap -->
    <script src="@Url.Content("~/Resources/js/bootstrap.min.js")" type="text/javascript"></script>
    <!-- FastClick -->
    <script src="@Url.Content("~/Plugins/fastclick/lib/fastclick.js")" type="text/javascript"></script>
    <!-- NProgress -->
    @*<script src="@Url.Content("~/Plugins/nprogress/nprogress.js")" type="text/javascript"></script>*@
    <!-- EasyAutocomplete-1.3.5 -->
    <script src="@Url.Content("~/Plugins/EasyAutocomplete-1.3.5/jquery.easy-autocomplete.js")" type="text/javascript"></script>
    <!-- Notify -->
    <script src="@Url.Content("~/Plugins/notify/pnotify.core.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Plugins/notify/pnotify.buttons.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Plugins/notify/pnotify.nonblock.js")" type="text/javascript"></script>
    <!-- jQuery custom content scroller -->
    <script src="@Url.Content("~/Plugins/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js")" type="text/javascript"></script>
    <!-- datatables -->
    <script src="@Url.Content("~/Plugins/datatables/datatables/jquery.dataTables.min.js")"></script>
    <script src="@Url.Content("~/Plugins/datatables/datatables/dataTables.bootstrap.js")"></script>
    <script src="@Url.Content("~/Plugins/datatables/datatables/dataTables.buttons.min.js")"></script>
    <script src="@Url.Content("~/Plugins/datatables/datatables/buttons.bootstrap.min.js")"></script>
    <!--
    <script src="@Url.Content("~/Plugins/datatables/datatables/jszip.min.js")"></script>
    <script src="@Url.Content("~/Plugins/datatables/datatables/pdfmake.min.js")"></script>
    <script src="@Url.Content("~/Plugins/datatables/datatables/vfs_fonts.js")"></script>
    -->
    <script src="@Url.Content("~/Plugins/datatables/datatables/buttons.html5.min.js")"></script>
    <script src="@Url.Content("~/Plugins/datatables/datatables/buttons.print.min.js")"></script>
    <script src="@Url.Content("~/Plugins/datatables/datatables/dataTables.fixedHeader.min.js")"></script>
    <script src="@Url.Content("~/Plugins/datatables/datatables/dataTables.keyTable.min.js")"></script>
    <script src="@Url.Content("~/Plugins/datatables/datatables/dataTables.responsive.min.js")"></script>
    <script src="@Url.Content("~/Plugins/datatables/datatables/responsive.bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Plugins/datatables/datatables/dataTables.scroller.min.js")"></script>
    <!-- 
    <script src="@Url.Content("~/Plugins/datatables/datatables/dataTables.cellEdit.js")"></script>
    -->
    <!-- Custom Theme Scripts -->
    <script src="@Url.Content("~/Resources/js/custom.js")" type="text/javascript"></script>
    <!-- gb.script.js -->
    <script src="@Url.Content("~/Resources/js/gb.script.js")" type="text/javascript"></script>
    <script type="text/javascript">
        // -- Réccupérer les données à exploiter dans les script du main -- //
        $GB_DONNEE_PARAMETRES = JSON.parse('@Html.Raw(ViewBag.GB_DONNEE)');
    </script>
    <!-- gb.menu.js -->
    <script src="@Url.Content("~/Resources/js/gb.menu.js")" type="text/javascript"></script>
    <!-- Script de la page -->
    <script src="@Url.Content("~/Resources/js/Views/" + ViewBag.Id_page + ".js")" type="text/javascript"></script>

</body>

</html>
